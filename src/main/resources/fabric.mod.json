{
    "schemaVersion": 1,
    "id": "slabee",
    "version": "${version}",
    "name": "Slabee",
    "description": "This is the ultimate slab block mod - New Slabs, Vertical Slabs, and Mixable Slabs!",
    "authors": [
        "Yurufu"
    ],
    "contact": {},
    "license": "MIT",
    "icon": "assets/slabee/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "com.forestotzka.yurufu.slabee.SlabeeClient"
        ],
        "main": [
            "com.forestotzka.yurufu.slabee.Slabee"
        ],
        "fabric-datagen": [
            "com.forestotzka.yurufu.slabee.SlabeeDataGenerator"
        ]
    },
    "mixins": [
        "slabee.mixins.json",
        {
            "config": "slabee.client.mixins.json",
            "environment": "client"
        }
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric": "*",
        "minecraft": "${minecraft_version}"
    }
}
